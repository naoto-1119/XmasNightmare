<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/screenBackground"
    android:visibility="visible"
    tools:context="com.xmsnm.xmasnightmare.ThirdPart2Fragment"
    tools:visibility="visible">

    <Button
        android:id="@+id/capture_button_santa_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/santa_second_round"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/santa_d3"
        app:layout_constraintVertical_bias="0.120000005" />

    <Button
        android:id="@+id/next_button_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/next_button"
        android:textSize="12sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accuracy_heart"
        app:layout_constraintVertical_bias="0.13" />

    <TextView
        android:id="@+id/accuracy_heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="@string/accuracy_heart"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/santa_d3"
        app:layout_constraintVertical_bias="0.060000002" />

    <TextView
        android:id="@+id/santa_d3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:width="300sp"
        android:text="@string/santa_d3"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/santa_image_2"
        app:layout_constraintVertical_bias="0.04000002" />

    <ImageView
        android:id="@+id/santa_image_2"
        android:layout_width="229dp"
        android:layout_height="225dp"
        android:background="@color/white"
        android:src="@drawable/m4mi8ehjpkrof19uohoavgte9i"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.13" />

    <ImageView
        android:id="@+id/santa_image_lock"
        android:layout_width="134dp"
        android:layout_height="119dp"
        android:src="@drawable/heartlock"
        app:layout_constraintBottom_toBottomOf="@+id/santa_image_2"
        app:layout_constraintEnd_toEndOf="@+id/santa_image_2"
        app:layout_constraintStart_toStartOf="@+id/santa_image_2"
        app:layout_constraintTop_toTopOf="@+id/santa_image_2" />
</androidx.constraintlayout.widget.ConstraintLayout>